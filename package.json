{
  "name": "@kronos-integration/interceptor-gzip",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/gunzip.mjs",
  "exports": {
    ".": "./src/gunzip.mjs"
  },
  "description": "interceptor to compress / decompress streams",
  "keywords": [
    "endpoint",
    "interceptor",
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API && documentation readme ./src/gunzip.mjs --section=API && documentation readme ./src/index.mjs --section=API && documentation readme ./src/decode-json.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/module.mjs && documentation lint ./src/gunzip.mjs && documentation lint ./src/index.mjs && documentation lint ./src/decode-json.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.2.0",
    "@kronos-integration/interceptor-stream": "^1.0.1",
    "model-attributes": "^4.1.7"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.7",
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-gzip.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-gzip/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-gzip#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
